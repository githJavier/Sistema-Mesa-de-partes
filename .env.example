# Dirección del servidor MySQL (por defecto 'db' en Docker Compose)
DB_HOST=

# Puerto del servidor MySQL (por defecto 3306)
DB_PORT=

# Nombre de la base de datos
DB_NAME=

# Usuario con acceso a la base de datos
DB_USER=

# Contraseña del usuario
DB_PASSWORD=

# Contraseña del usuario root (si se usa)
DB_ROOT_PASSWORD=

# URL del proyecto Supabase (lo obtienes desde tu dashboard)
SUPABASE_URL=

# Clave pública anónima (anon key) desde API > Project API keys
SUPABASE_ANON_KEY=