# Dirección del servidor MySQL (por defecto 'db' si usas Docker Compose)
DB_HOST=

# Puerto en el que escucha MySQL (por defecto 3306)
DB_PORT=

# Nombre de la base de datos que utilizará la aplicación
DB_NAME=

# Usuario con permisos para acceder a la base de datos
DB_USER=

# Contraseña del usuario de base de datos
DB_PASSWORD=

# Contraseña del usuario root de MySQL (solo si aplica)
DB_ROOT_PASSWORD=

# URL del proyecto en Supabase (obtenido desde el dashboard del proyecto)
SUPABASE_URL=

# Clave pública anónima para acceder al API de Supabase (sección API > Project API keys)
SUPABASE_ANON_KEY=

# Token de autenticación para la API de Factiliza (usado en consultas de DNI/RUC)
FACTILIZA_TOKEN=
